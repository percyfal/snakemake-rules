# -*- snakemake -*-
include: "star.settings"

rule star_align_log:
    input: bam = "{prefix}.Aligned.out.bam"
    output: log = "{prefix}.Log.final.out"
    shell: "touch {output.log}"


rule star_align_log_sorted:
    input: bam = "{prefix}.Aligned.sortedByCoord.out.bam"
    output: log = "{prefix}.Log.final.out"
    shell: "touch {output.log}"
